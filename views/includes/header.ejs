<header>
  <nav id="navbar">
    <ul id="navbar-list">
      <% if (locals.isAdmin) { %>
      <li><a href="/dashboard" class="nav-link">Dashboard</a></li>
      <li><a href="/admin" class="nav-link">Admin</a></li>
      <% } %>
      <% if (!locals.isAuth) { %>
      <li><a href="/login" class="nav-link">Login</a></li>
      <% } %>
      <% if (locals.isAuth) { %>
        <li><a href="/capture" class="nav-link">Capture</a></li>
        <li>
          <form action="/logout" method="POST">
            <button class="logout"><i class="fa-solid fa-power-off"></i></button>
          </form>
        </li>
        <% } %>
      
    </ul>
  </nav>
</header>